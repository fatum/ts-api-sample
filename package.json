  "scripts": {
    "operation": "yarn ts-node examples/run.ts -e ts",
    "poly": "yarn ts-node examples/polymorphic.ts -e ts",
    "precommit": "yarn pretty-quick --staged",
    "format": "yarn pretty-quick",

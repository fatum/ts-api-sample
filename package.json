    "@types/validator": "^12.0.1",
    "apollo-server-express": "^2.14.2",
    "bull": "^3.13.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dataloader": "^2.0.0",
    "decimal.js": "^10.2.0",
    "express": "^4.17.1",
    "graphql": "^14.6.0",
    "mathjs": "^6.6.1",
    "pg": "^7.18.2",
    "pg-hstore": "^2.3.3",
    "sequelize": "^5.21.5",
    "typescript": "^3.8.3",
    "validator": "^13.0.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^1.13.1",
    "@graphql-codegen/introspection": "1.13.1",

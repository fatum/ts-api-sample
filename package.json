    "@types/bull": "^3.12.1",
    "@types/compression": "^1.7.0",
    "@types/express": "^4.17.3",
    "@types/graphql": "^14.5.0",
    "graphql-depth-limit": "^1.1.0",
    "graphql-import": "^1.0.1",
    "graphql-import-node": "^0.0.4",
    "nodemon": "^2.0.2",
    "prettier": "^2.0.2",
    "pretty-quick": "^2.0.1",
    "sequelize-cli": "^5.5.1",
    "ts-node": "^8.8.1"
  }
}
